{
  "name": "AthenaAnnotate",
  "version": "0.0.1",
  "description": "Annotate the web.",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build",
    "mkdir": "mkdir -p ./build/extension ./build/athena",
    "copy": "npm run mkdir && cp src/libs/athena/src/assets/markup/athena.html ./build/athena && cp src/libs/extension/src/manifest.json ./build/extension",
    "build-app": "webpack --progress",
    "build-ext": "webpack --config src/libs/extension/webpack.config.js --progress",
    "build-zeus": "webpack --config src/libs/zeus/webpack.config.js --progress",
    "build-athena": "webpack --config src/libs/athena/webpack.config.js --progress",
    "build": "npm run build-app && npm run build-ext && npm run build-zeus && npm run build-athena && npm run copy",
    "watch-app": "npm run build-app -- --watch",
    "watch-ext": "npm run build-ext -- --watch",
    "watch-zeus": "npm run build-zeus -- --watch",
    "watch-athena": "npm run build-athena -- --watch",
    "start": "node src/server/server.js",
    "test": "",
    "lint": "eslint src --color",
    "drop": "dropdb -U postgres annotate",
    "create": "createdb -U postgres annotate && node_modules/.bin/sequelize db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AthenaAnnotate/AthenaAnnotate.git"
  },
  "bugs": {
    "url": "https://github.com/AthenaAnnotate/AthenaAnnotate/issues"
  },
  "homepage": "https://github.com/AthenaAnnotate/AthenaAnnotate#readme",
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.1",
    "bootstrap": "^3.3.6",
    "cheerio": "^0.20.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "es6-promise": "^3.2.1",
    "express": "^4.14.0",
    "express-session": "^1.13.0",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.7.0",
    "pg": "^6.0.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.5",
    "react-dom": "^15.1.0",
    "react-modal": "^1.4.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "request": "^2.72.0",
    "request-image-size": "git://github.com/Elbertlum/request-image-size.git",
    "sequelize": "^3.23.3",
    "sequelize-cli": "^2.4.0"
  }
}
